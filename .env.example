# LLM Configuration
AZURE_OPENAI_LLM_ENDPOINT=https://aiportalapi.stu-platform.live/jpe
AZURE_OPENAI_LLM_API_KEY=your_azure_llm_key_here
AZURE_OPENAI_LLM_MODEL=GPT-4o

# Embeddings Configuration
AZURE_OPENAI_EMBEDDING_ENDPOINT=https://aiportalapi.stu-platform.live/jpe
AZURE_OPENAI_EMBEDDING_API_KEY=your_azure_embedding_key_here
AZURE_OPENAI_EMBED_MODEL=text-embedding-3-small

# Optional - Tavily Search API
TAVILY_API_KEY=your_tavily_key_here

# Gemini API Key (Primary LLM)
GEMINI_API_KEY=your_gemini_api_key_here

# LLM Provider Settings
LLM_PROVIDER=gemini
LLM_MODEL=gemini-2.5-flash
TEMPERATURE=0.5
MAX_TOKENS=4000
